<h1><%= @album.name %></h1>
<h4>Artist: <%= @album.artist %></h4>
<h4><strong>Ranking: <%= @album.rank %></strong></h4>
<p>
  <%= @album.description %>
</p>
<%= button_to "Upvote", album_voteup_path(@album.id) %>
<%= link_to "Edit #{@album.name}", edit_album_path(@album.id) %>
<%= button_to "Delete", album_path(@album.id), method: :delete %>
<%= link_to "View All Albums", albums_path %>
<%= link_to "View All Media", root_path %>
